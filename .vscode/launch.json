  "version": "0.2.0",
  "envFile": "${workspaceFolder}/.env",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'qbiz-api'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=qbiz-api"
        ],
        "filter": {
          "name": "qbiz-api",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug example 'common_bank_list'",
      "cargo": {
        "args": [
          "build",
          "--example=common_bank_list",
          "--package=qbiz-api"
        ],
        "filter": {
          "name": "common_bank_list",
          "kind": "example"
        }
      },
      "args": [],
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}"
    },
  ]
}